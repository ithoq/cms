<body class="fixed-header">
<div class="modal fade slide-up disable-scroll in" id="modalSession" tabindex="-1" role="dialog" aria-hidden="false">
    <div class="modal-dialog modal-sm">
        <div class="modal-content-wrapper">
            <div class="modal-content">
                <div class="modal-body text-center m-t-20">
                    <h3>{{ message('session expiration') | capitalize }}</h3>
                    <p>{{ message('session.will.expire') | capitalize }}</p>
                    <span id="sessionCounter" class="sessionCounter"></span>
                    <button type="button" id="sessionKeepAlive" class="btn btn-primary btn-cons" data-dismiss="modal">{{ message('stay connected') | capitalize }}</button>
                </div>
            </div>
        </div>
    </div>
</div>
{% if noSidebar == null or not noSidebar %}
    {% include 'admin/include/sidebar' %}
{% endif %}

<div class="page-container {{ pageContentClass }}">
    {% if noHeader == null or not noHeader %}
        {% include 'admin/include/header' %}
    {% endif %}

    <div class="page-content-wrapper">
        {% block modal %}
        {% endblock modal %}
        <div class="content">
            {% if noBreadCrumb == null or not noBreadCrumb %}
                <div data-pages="parallax" class="jumbotron">

                    <div class="container-fluid container-fixed-lg sm-p-l-20 sm-p-r-20">
                        <div class="inner">
                            <ul class="breadcrumb">
                                <li>
                                    <p>{{ title }}</p>
                                </li>
                                {% if subtitle is not empty %}
                                    <li><a href="#" class="active">{{ subtitle }}</a></li>
                                {% endif %}
                            </ul>
                        </div>
                    </div>
                </div>

            {% endif %}

            <div class="container-fluid container-fixed-lg" id="main-content">
                {% block content %}
                {% endblock content %}
            </div>
        </div>
    </div>
    {% if noFooter == null or not noFooter %}
        {% include 'admin/include/footer' %}
    {% endif %}

</div>
<script src="/resources/cms/libs/pages/js/pages.js"></script>
<script src="/resources/cms/js/main.js"></script>

{% block bottom %}
{% endblock bottom %}
</body>
